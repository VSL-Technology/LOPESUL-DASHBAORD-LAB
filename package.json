{
  "type": "module",
  "name": "lopesul-dashboard",
  "version": "1.0.0",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "next dev --port 3000 --hostname 0.0.0.0",
    "build": "npm run check:runtime-guard && next build",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/app/api/**/*.{js,ts,tsx}\" \"src/lib/**/*.{js,ts,tsx}\"",
    "format": "prettier --write src/",
    "postinstall": "prisma generate",
    "prestart": "prisma migrate deploy || prisma db push",
    "start": "next start -H 0.0.0.0 -p ${PORT:-3000}",
    "start:local": "next start --port 3000 --hostname 0.0.0.0",
    "studio": "prisma studio --port 5555 --hostname 0.0.0.0",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "node prisma/seed.js",
    "db:push": "prisma db push",
    "prisma:generate": "prisma generate",
    "prisma:validate": "prisma validate",
    "create:test-user": "node scripts/criar-usuario-teste.js",
    "populate:identity": "node scripts/populate-router-identity.mjs",
    "check:runtime-guard": "bash scripts/ci/deny_runtime_network_and_routeros.sh"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "@types/react": "19.2.2",
    "autoprefixer": "^10.4.0",
    "bcryptjs": "^2.4.3",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.2",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.513.0",
    "mikronode": "^2.0.5",
    "mikronode-ng2": "^1.0.12",
    "next": "^15.5.9",
    "node-fetch": "^3.3.2",
    "node-routeros": "^1.6.8",
    "node-ssh": "^13.2.1",
    "ping": "^0.4.4",
    "pino": "^10.1.0",
    "postcss": "^8.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-qr-code": "^2.0.18",
    "recharts": "^3.1.0",
    "source-map-support": "^0.5.21",
    "tailwindcss": "^3.3.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/pino": "^7.0.4",
    "@types/react": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "baseline-browser-mapping": "^2.9.11",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "prettier": "^3.7.4",
    "prisma": "^6.16.3",
    "supports-color": "^10.2.2",
    "typescript": "^5.9.3"
  }
}
